            </Slider>
            <Slider fx:id="soundeffekteS" majorTickUnit="0.25" max="1.0" showTickMarks="true" value="1.0" GridPane.rowIndex="6">
                <GridPane.margin>
                    <Insets left="20.0" right="20.0" />
                </GridPane.margin>
            </Slider>
            <Label text="Audio">
                <font>
                    <Font name="System Bold" size="18.0" />
                </font>
            </Label>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.rowIndex="8">
                <buttons>
                    <Button maxWidth="50.0" mnemonicParsing="false" onAction="#handleOK" text="OK" />
                    <Button maxWidth="100.0" mnemonicParsing="false" onAction="#handleAbbrechen" text="Abbrechen" />
                </buttons>
            </ButtonBar>

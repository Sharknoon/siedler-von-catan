<StackPane style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.sharknoon.siedlervoncatan.view.controller.AudioController">
    <AnchorPane maxHeight="400.0" maxWidth="450.0" prefHeight="400.0" prefWidth="450.0" styleClass="pergamentrolle" stylesheets="@stylesheet.css">
        <GridPane hgap="5.0" layoutX="200.0" layoutY="255.0" vgap="5.0" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="55.0" AnchorPane.rightAnchor="55.0" AnchorPane.topAnchor="55.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <CheckBox fx:id="musikAnCB" mnemonicParsing="false" selected="true" text="Musik An" GridPane.halignment="LEFT" GridPane.rowIndex="2">
                <GridPane.margin>
                    <Insets left="20.0" />
                </GridPane.margin>
            </CheckBox>
            <CheckBox fx:id="soundeffekteAnCB" mnemonicParsing="false" selected="true" text="Soundeffekte An" GridPane.rowIndex="5">
                <GridPane.margin>
                    <Insets left="20.0" />
                </GridPane.margin>
            </CheckBox>
            <Slider fx:id="musikS" majorTickUnit="0.25" max="1.0" showTickMarks="true" value="1.0" GridPane.rowIndex="3">
